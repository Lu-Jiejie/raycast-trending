{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "trending-topics",
  "title": "Trending Topics",
  "type": "module",
  "description": "A Raycast extension for exploring trending topics.",
  "author": "Lu-Jiejie",
  "license": "MIT",
  "categories": [
    "Productivity"
  ],
  "platforms": [
    "Windows"
  ],
  "icon": "icon.png",
  "commands": [
    {
      "name": "trending-topics",
      "title": "Trending Topics",
      "description": "Explore trending topics",
      "mode": "view"
    }
  ],
  "preferences": [],
  "scripts": {
    "dev": "ray develop",
    "build": "ray build -e dist",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "preferences": "tsx ./scripts/preferences.ts"
  },
  "dependencies": {
    "@raycast/api": "^1.103.1",
    "@raycast/utils": "^2.2.1",
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.13",
    "eslint": "^9.35.0",
    "lint-staged": "^16.1.6",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
